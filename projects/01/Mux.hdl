/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=c);
    And(a=a,b=c,out=d1);
    And(a=b,b=sel,out=d2);
    Or(a=d1,b=d2,out=out);
}